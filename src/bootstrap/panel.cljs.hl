(ns bootstrap.panel
  (:refer-hoplon :exclude [body title heading])
  
  )

(defelem default  [attr kids] (apply (div :class "panel panel-default") attr kids))
(defelem info     [attr kids] (apply (div :class "panel panel-info") attr kids))
(defelem body     [attr kids] (apply (div :class "panel-body") attr kids))
(defelem heading  [attr kids] (apply (div :class "panel-heading") attr kids))
(defelem title    [attr kids] (apply (h3 :class "panel-title") attr kids))
