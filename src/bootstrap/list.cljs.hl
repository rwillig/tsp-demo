(ns bootstrap.list
  
  )

(defelem group    [attr kids] (apply (ul :class "list-group") attr kids))
(defelem item     [attr kids] (apply (li :class "list-group-item") attr kids))
(defelem divgroup [attr kids] (apply (div :class "list-group") attr kids))
(defelem divitem  [attr kids] (apply (div :class "list-group-item") attr kids))
